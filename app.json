{
	"expo": {
		"name": "oech-app",
		"slug": "oech-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/main/res/drawable/app-icon-rectangle.png",
		"userInterfaceStyle": "light",
		"platforms": ["ios", "android"],
		"splash": {
			"image": "./src/main/res/drawable/splash-transparent.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/main/res/drawable/app-icon-circle.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./src/main/res/drawable/app-icon-circle.png"
		}
	}
}
